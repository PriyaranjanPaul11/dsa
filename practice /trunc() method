#include <bits/stdc++.h>
using namespace std;
int main() {
   
   double x=2.333,y=5.273,z=1.2233;
   
   cout<<trunc(x)<<endl;
   cout<<trunc(y)<<endl;
   cout<<trunc(z)<<endl;
   
   double a=-2.333,b=-5.273,c=-1.2233;
   
   cout<<trunc(a)<<endl;
   cout<<trunc(b)<<endl;
   cout<<trunc(c)<<endl;

    return 0;
}
