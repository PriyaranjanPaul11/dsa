#include <bits/stdc++.h>
using namespace std;
int main() {
   
   double x=2.333,y=5.273,z=1.2233;
   
   cout<<floor(x)<<endl;
   cout<<floor(y)<<endl;
   cout<<floor(z)<<endl;
   
   double a=-2.333,b=-5.273,c=-1.2233;
   
   cout<<floor(a)<<endl;
   cout<<floor(b)<<endl;
   cout<<floor(c)<<endl;

    return 0;
}
