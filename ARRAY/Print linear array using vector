#include <iostream>
#include <vector>
#include <limits.h>
using namespace std;

int main(){
   vector<int> arr;
   
   


 
   arr.push_back(4);
   arr.push_back(8);
   arr.push_back(12);
   arr.push_back(11);
   arr.push_back(4);

   cout << arr.size() << endl;

   cout << arr.empty() << endl;
  

   cout << "Size of vector: " << arr.size();

   cout << endl << "Printing vector" << endl;
   for(int i=0; i<arr.size(); i++) {
    cout << arr[i] <<" ";
   }cout << endl;
}
