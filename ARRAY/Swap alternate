#include <iostream>
using namespace std;


void swapAlternate(int arr[],int size){
    for(int i=0;i<size;i=i+2){
        swap(arr[i],arr[i+1]);
    }
}

int main(){
    int arr[6]={1,2,3,4,5,6};
    int n=6;
    
    swapAlternate(arr,6);
    
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
}
