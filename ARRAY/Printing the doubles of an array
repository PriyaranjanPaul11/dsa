#include <iostream>
using namespace std;

int main() {
  int arr[10];

  int n=10;
  for(int i=0;i<n;i++){
    
    cin>>arr[i];
    cout<<endl;
  }

  cout<<"Printing the array after taking input"<<endl;

  for(int i=0;i<n;i++){
      
    cout<<arr[i]<<" ";
  }
  cout<<endl;
  cout<<"Printing the Doubles"<<endl;
  for(int i=0;i<n;i++){
      arr[i]=2*arr[i];
    cout<<arr[i]<<" ";
  }
  return 0;
}
